{
  "name": "auto-test-new",
  "version": "0.30.0",
  "description": "Auto release fresh release single package",
  "main": "index.js",
  "source": "index.js",
  "author": {
    "name": "Andrew Lisowski",
    "email": "lisowski54@gmail.com"
  },
  "scripts": {
    "test": "jest",
    "lint": "xo",
    "release": "auto shipit"
  },
  "devDependencies": {
    "auto-release-cli": "^0.37.9",
    "eslint-config-prettier": "^3.3.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "xo": "^0.23.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "xo": {
    "env": [
      "browser",
      "jest"
    ],
    "extends": [
      "prettier"
    ]
  }
}
